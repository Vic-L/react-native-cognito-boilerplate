{
  "name": "ReactNativeCognitoBoilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "prepare": "cd node_modules/react-native ; ./scripts/ios-install-third-party.sh ; cd ../../ && cd node_modules/react-native/third-party/glog-0.3.*/ ; ./configure ; cd ../../../../",
    "iphoneX": "ENVFILE=.env.development react-native run-ios --simulator='iPhone X' # --scheme CustomScheme",
    "iphonePlus": "ENVFILE=.env.development react-native run-ios --simulator='iPhone 6 Plus' # --scheme CustomScheme",
    "android": "cd android && ./gradlew clean && cd .. && react-native run-android",
    "test": "jest"
  },
  "dependencies": {
    "@aws-amplify/auth": "^1.1.2",
    "apollo-cache-inmemory": "^1.2.10",
    "apollo-client": "^2.4.2",
    "apollo-link-http": "^1.5.5",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "lodash": "^4.17.11",
    "prop-types": "^15.6.2",
    "react": "16.4.1",
    "react-apollo": "^2.1.11",
    "react-apollo-network-status": "^1.1.0",
    "react-native": "0.56.1",
    "react-native-config": "^0.11.5",
    "react-native-firebase": "^4",
    "react-native-image-crop-picker": "^0.21.2",
    "react-native-keyboard-aware-scroll-view": "^0.7.2",
    "react-native-splash-screen": "^3.1.1",
    "react-native-status-bar-height": "^2.1.0",
    "react-navigation": "^2.13.0",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-saga": "^0.16.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "babel-jest": "23.6.0",
    "babel-preset-react-native": "^5",
    "jest": "23.6.0",
    "react-test-renderer": "16.4.1"
  },
  "jest": {
    "preset": "react-native"
  }
}
